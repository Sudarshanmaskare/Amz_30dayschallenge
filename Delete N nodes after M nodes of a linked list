 void linkdelete(struct Node  *head, int M, int N)
    {
        //Add code here   
        // Function to skip M nodes and then
// delete N nodes of the linked list
   Node *cur=head;
   Node *t;
   int count;
   while(cur){
       for(count=1;count<M && cur!=NULL;count++){
           cur=cur->next;
       }
       if(cur==NULL){
           return;
       }
       t=cur->next;
       for(count=1;count<=N && t!=NULL;count++){
           Node *temp=t;
           t=t->next;
           delete temp;
       }
       cur->next=t;
       cur=t;
       
   }
