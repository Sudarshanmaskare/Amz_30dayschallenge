vector <int> max_of_subarrays(int *arr, int n, int k)
    {
       if(n==0) return {};
       deque<int> dq;
       vector<int> ans;
       for(int i=0;i<n;i++){
           if(dq.size() && i-k==dq.front()) dq.pop_front();
           while(dq.size() && arr[dq.back()]<=arr[i]) dq.pop_back();
           dq.push_back(i);
           if(i>=k-1) ans.push_back(arr[dq.front()]);
       }
       return ans;
    }
