string FirstNonRepeating(string A){
		    // Code here
		    if(A.size()==0) return "";
		    int arr[26]={0};
		    
		    queue<char> q;
		    string ans="";
		    for(int i=0;i<A.size();i++){
		        arr[A[i]-'a']++;
		        q.push(A[i]);
		        while(q.size()){
		            if(arr[q.front()-'a']>1) q.pop();
		            else break;
		        }
		        if(q.size()){
		            ans+=q.front();
		        }
		        else ans+="#";
		    }
		    return ans;
		}
