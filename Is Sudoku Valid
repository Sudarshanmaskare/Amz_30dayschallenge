int isValid(vector<vector<int>> mat){
        // code here
        unordered_set<string> mp;
        for(int i=0;i<9;i++){
            for(int j=0;j<9;j++){
                if(mat[i][j]==0) continue;
                
                string row=to_string(mat[i][j])+to_string(i)+"row";
                string col=to_string(mat[i][j])+to_string(j)+"col";
                string box=to_string(mat[i][j])+to_string(i/3)+to_string(j/3);
                
                if(mp.count(row) || mp.count(col) || mp.count(box)) return false;
                mp.insert(row);
                mp.insert(col);
                mp.insert(box);
                
            }
        }
        return true;
    }
